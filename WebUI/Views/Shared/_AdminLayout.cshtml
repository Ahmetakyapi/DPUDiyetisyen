<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>DPU Diyetisyen Admin Paneli</title>
    <link rel="stylesheet" href="~/wwwroot/Admin/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendors/base/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/css/style.css">
    <link rel="shortcut icon" href="" />
</head>
<body>
    <div class="container-scroller">

        <!-- partial:partials/_horizontal-navbar.html -->
        <div class="horizontal-menu">

            <nav class="navbar top-navbar col-lg-12 col-12 p-0">
                <div class="container-fluid">
                    <div class="navbar-menu-wrapper d-flex align-items-center justify-content-between">

                        <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
                            <a class="navbar-brand brand-logo" href="/Admin/Dashboard"><img src="~/wwwroot/Admin/images/logo.svg" alt="logo" /></a>
                            <a class="navbar-brand brand-logo-mini" href="/Admin/Dashboard"><img src="~/wwwroot/Admin/images/logo-mini.svg" alt="logo" /></a>
                        </div>
                        <ul class="navbar-nav navbar-nav-right">

                            <li class="nav-item nav-profile dropdown">
                                <a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" id="profileDropdown">
                                    <span class="nav-profile-name">Ahmet Akyapı</span>
                                    <span class="online-status"></span>
                                    <img src="~/wwwroot/Admin/images/faces/face28.png" alt="profile" />
                                </a>
                                <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="profileDropdown">
                                    <a class="dropdown-item">
                                        <i class="mdi mdi-settings text-primary"></i>
                                        Ayarlar
                                    </a>
                                    <a class="dropdown-item">
                                        <i class="mdi mdi-logout text-primary"></i>
                                        Çıkış
                                    </a>
                                </div>
                            </li>
                        </ul>
                        <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="horizontal-menu-toggle">
                            <span class="mdi mdi-menu"></span>
                        </button>
                    </div>
                </div>
            </nav>

            <nav class="bottom-navbar">
                <div class="container">
                    <ul class="nav page-navigation">
                        <li class="nav-item">
                            <a class="nav-link" href="/Admin/Article">
                                <i class="mdi mdi-file-document-box menu-icon"></i>
                                <span class="menu-title">Makaleler</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Recipes/Home" class="nav-link">
                                <i class="mdi mdi-cube-outline menu-icon"></i>
                                <span class="menu-title">Tarifler</span>
                                <i class="menu-arrow"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Proposal/Home" class="nav-link">
                                <i class="mdi mdi-chart-areaspline menu-icon"></i>
                                <span class="menu-title">Öneriler</span>
                                <i class="menu-arrow"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/About/Index" class="nav-link">
                                <i class="mdi mdi-finance menu-icon"></i>
                                <span class="menu-title">Hakkımda</span>
                                <i class="menu-arrow"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Admin/Contact" class="nav-link">
                                <i class="mdi mdi-grid menu-icon"></i>
                                <span class="menu-title">İletişim</span>
                                <i class="menu-arrow"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>

        <div>
            @RenderBody()
        </div>
        <!-- partial -->
        <div class="container-fluid ">
            <div class="main-panel">

                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="footer-wrap">
                        <div class=">
                            <span class="d-block text-center text-sm-left d-sm-inline-block">Copyright © 2020. All rights reserved.</span>
                            <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Ahmet Akyapı <i class="mdi mdi-heart-outline"></i></span>
                        </div>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>


        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- base:js -->
    <script src="~/wwwroot/Admin/vendors/base/vendor.bundle.base.js"></script>
    <script src="~/wwwroot/Admin/js/template.js"></script>
    <script src="~/wwwroot/Admin/vendors/chart.js/Chart.min.js"></script>
    <script src="~/wwwroot/Admin/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/wwwroot/Admin/vendors/chartjs-plugin-datalabels/chartjs-plugin-datalabels.js"></script>
    <script src="~/wwwroot/Admin/vendors/justgage/raphael-2.1.4.min.js"></script>
    <script src="~/wwwroot/Admin/vendors/justgage/justgage.js"></script>
    <script src="~/wwwroot/Admin/js/dashboard.js"></script>
</body>
</html>
